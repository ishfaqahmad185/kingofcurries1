@{
    ViewData["Title"] = "Resturant Closed";
}


@if (!string.IsNullOrEmpty(ViewBag.ErrorMsg))
{

    <section class="space">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <img src="@ViewBag.ErrorMsg" />
                </div>
            </div>
            <div class="row m-4">
                <div class="col-4"></div>
                <div class="col-4"><a href="/" class="btn btn-primary w-100"><i class="fa fa-home"></i> Go To Home</a></div>
                <div class="col-4"></div>

            </div>
        </div>

    </section>
}
