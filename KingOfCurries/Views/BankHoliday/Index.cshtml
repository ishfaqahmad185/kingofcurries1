@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div id="body_holder">
    <div class="row m-0 mb-3">
        <div class="col-lg-12">
            <div class="alert alert-success" id="addBankHolidaySuccessMessage" hidden>
            </div>

        </div>

    </div>

    <div class="row m-0 mb-3">
        <div class="col-lg-12">
            <div class="alert alert-danger" id="addBankHolidayErrorMessage" hidden>
            </div>

        </div>

    </div>


    <div class="card">
        <div class="card-header card-header-custom">
            <a id="AddNew" class="btn btn-outline-warning btn-custom btn-sm float-right" onclick="AddBankHolidays()" title="Add New"> <i class="fa fa-plus"></i> Add BankHoliday</a>
            Manage BankHoliday
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-sm-12">
                    @await Html.PartialAsync("~/Views/BankHoliday/_ListBankHoliday.cshtml")
                </div>
             </div>
        </div>
    </div>




</div>